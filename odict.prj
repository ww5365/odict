target(odict)
{
	TEMPLATE = lib
        SOURCES = odict.cpp                       
        all(output){
        SHELL=rm -rf output
        SHELL+=mkdir -p output/include
        SHELL+=mkdir -p output/demo
        SHELL+=cp libodict.a output
        SHELL+=cp odict.h output/include
        SHELL+=cp main*.cpp output/demo
        SHELL+=cp README output/
        SHELL+=cp ChangeLog output/
        }
}

