WORKROOT = ../../
INCPATH += .
#设置MAKDEP参数，会让你的编译更安全 
CONFIG += MAKDEP
#填写编译依赖, 路径
linux32 {
	CONFIG += lib2/ullib 
	CONFIG += lib2/dict
}
linux64 {
	CONFIG += lib2-64/ullib 
	CONFIG += lib2-64/dict
}
#填写预编译参数
CPPFLAGS += -DVERSION=\"1.1.4.2\" -DLINUX -D_REENTRANT
#填写CXX的编译参数
CXXFLAGS += -g -Winline  -Wall -pipe -fPIC -O2 -finline-functions
#填写链接库
LDLIBS += -lpthread
#连接参数
LINKFLAGS += -rdynamic

